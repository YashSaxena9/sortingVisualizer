{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": "off",
    "no-await-in-loop": "off",
    "no-loop-func": "off",
    "consistent-return": "off",
    "import/extensions": "off"
  }
}
